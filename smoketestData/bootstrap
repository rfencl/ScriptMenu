#!/bin/bash

#-----------------------------------------------------------------------------------------------
# Working directory 
#-----------------------------------------------------------------------------------------------
SMOKETESTDIR="/home/powin/tools/smoketestData/"
#-----------------------------------------------------------------------------------------------
# clear circular load blocks
#-----------------------------------------------------------------------------------------------
clearLoadSemaphores () {
  unset SHAREDVARS_G
  unset GENERALFUNCTIONS_G
}
#-----------------------------------------------------------------------------------------------
# load support files
#-----------------------------------------------------------------------------------------------
loadSupportFiles () {
. ${SMOKETESTDIR}generalFunctions   
}

#-----------------------------------------------------------------------------------------------
# Application setup
#-----------------------------------------------------------------------------------------------

  clearLoadSemaphores
  loadSupportFiles

