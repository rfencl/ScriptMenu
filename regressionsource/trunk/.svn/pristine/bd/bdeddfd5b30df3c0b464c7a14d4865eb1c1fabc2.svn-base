package com.powin.modbusfiles.configuration;

public class ConfigurationParameters {
	private String stationName;
	private int cStringCount;
	private int cContainerIndex;
	private int cArrayCount;
	private int cArrayStartIndex;
	private StackType cStackType;

	public ConfigurationParameters() {

	}

	public ConfigurationParameters(String stationName,StackType stackType, int stringCount, int containerIndex, int arrayCount, int arrayIndex) {
		
		setStationName(stationName);
		setStringCount(stringCount);
		setContainerIndex(containerIndex);
		setArrayCount(arrayCount);
		setArrayStartIndex(arrayIndex);
		setStackType(stackType);
	}

	public String getStationName() {
		return stationName;
	}

	public void setStationName(String stationName) {
		this.stationName = stationName;
	}

	public void setStackType(StackType stackType) {
	   this.cStackType = stackType;
	}
	
	public StackType getStackType() {
		return cStackType;
	}

	public int getStringCount() {
		return cStringCount;
	}

	public void setStringCount(int stringCount) {
		cStringCount = stringCount;
	}

	public int getContainerIndex() {
		return cContainerIndex;
	}

	public void setContainerIndex(int containerIndex) {
		cContainerIndex = containerIndex;
	}

	public int getArrayCount() {
		return cArrayCount;
	}

	public void setArrayCount(int arrayCount) {
		cArrayCount = arrayCount;
	}

	public int getArrayStartIndex() {
		return cArrayStartIndex;
	}

	public void setArrayStartIndex(int arrayStartIndex) {
		cArrayStartIndex = arrayStartIndex;
	}
}
